<!DOCTYPE html>
<html>
<head>
  <title>CountryInfo</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div ng-app="countryInfo" ng-controller="CountryController">

    <div ng-repeat='country in country'>
      {{ country.name }} - {{ country.population }}
    </div>

  </div>



</body>
</html>
